{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
  ]
%}
{% if content %}
  <div{{ attributes.addClass(classes) }}>
    <div class="container">
        <div class="container-frame inlineframe">
            <div class="footer-logo">
                {{ content }}
                <!-- <div class="copyright">
                    &copy; TASA, {{ "now"|date("Y") }}
                </div> -->
            </div>
            <div class="footer-details">
                <div class="detail address-detail">
                    <div class="detail-icon">
                        <i class="fa fa-location-arrow" aria-hidden="true"></i>
                    </div>
                    <div class="detail-text">
                        <h4>Contact Us:</h4>
                        {{ drupal_entity('block_content', '6') }}
                    </div>
                </div>
                <div class="detail contact-detail">
                    <div class="email">
                        <div class="detail-icon">
                            <i class="fa fa-envelope-o" aria-hidden="true"></i>
                        </div>
                        <div class="detail-text">
                        {{ drupal_entity('block_content', '3') }}
                      </div>
                    </div>
                    <div class="phone">
                        <div class="detail-icon">
                            <i class="fa fa-phone" aria-hidden="true"></i>
                        </div>
                        <div class="detail-text">
                        {{ drupal_entity('block_content', '4') }}
                        </div>
                    </div>
                    <div class="social-icons">
                      {{ drupal_entity('block_content', '5') }}
                    </div>
                </div>
            </div>
        </div>
    </div>
  </div>
{% endif %}
